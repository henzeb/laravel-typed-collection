{
  "name": "henzeb/laravel-typed-collection",
  "description": "Laravel typed collections",
  "keywords": [
    "henzeb",
    "laravel",
    "generic",
    "generics",
    "typed",
    "collection",
    "collections",
    "eloquent",
    "type-safe",
    "typesafe",
    "type",
    "safe"
  ],
  "homepage": "https://github.com/henzeb/laravel-typed-collection",
  "license": "AGPL-3.0-only",
  "type": "library",
  "authors": [
    {
      "name": "Henze Berkheij",
      "email": "henzeberkheij@gmail.com",
      "role": "Developer"
    }
  ],
  "suggest": {
    "ramsey/uuid": "^4.2.2",
    "symfony/uid": "^5.1|^6.3|^7"
  },
  "require": {
    "php": "^8.1",
    "illuminate/collections": "^10|^11|^12"
  },
  "require-dev": {
    "calebporzio/sushi": "^2.4",
    "orchestra/testbench": "^8|^9|^10",
    "phpstan/phpstan": "^1.4",
    "phpunit/phpunit": "^9.6.6|^10",
    "ramsey/uuid": "^4.2.2",
    "symfony/uid": "^5.1|^6.3|^7"
  },
  "autoload": {
    "psr-4": {
      "Henzeb\\Collection\\": "src/",
      "Henzeb\\Collection\\Tests\\": "tests/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Henzeb\\Collection\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage-txt": "XDEBUG_MODE=coverage vendor/bin/phpunit --coverage-text",
    "test-coverage": "XDEBUG_MODE=coverage vendor/bin/phpunit --coverage-html coverage",
    "test-dox": "vendor/bin/phpunit --testdox",
    "infection": "vendor/bin/infection --git-diff-filter=AM"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "infection/extension-installer": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Henzeb\\Collection\\Providers\\TypedCollectionProvider"
      ]
    }
  }
}
